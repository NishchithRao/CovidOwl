{
  "expo": {
    "name": "CovidOwl",
    "slug": "CovidOwl",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.devmonk.covidowl",
      "supportsTablet": true
    },
    "android": {
      "package": "com.devmonk.covidowl"
    }
  }
}
